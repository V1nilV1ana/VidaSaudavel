  const lastPostId =  Posts.max('id')
  const lastId = Posts.findByPk(lastPostId)

  Posts.create(post)
    
    .then(data => {
      Posts.addcreatorPosts([lastId, req.query.userId])  
      res.send(data);
    })
    .catch(err => {
      res.status(500).send({
        message:
          err.message || "Erro enquantos os posts eram recuperados"
      });
    });
};
